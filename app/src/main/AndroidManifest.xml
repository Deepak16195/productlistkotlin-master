<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.demosample.mvvm"
          xmlns:android="http://schemas.android.com/apk/res/android">

  <uses-permission android:name="android.permission.INTERNET"/>
  <application
      android:name="com.demosample.MvvmApp"
      android:allowBackup="true"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name="com.demosample.mvvm.ui.main.CreateProductActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>

    <activity android:name="com.demosample.mvvm.ui.main.MainActivity"></activity>
    <activity android:name="com.demosample.mvvm.ui.main.UpdateProductActivity">

    </activity>
  </application>
</manifest>